{
    "manifest_version": 2,
    "name": "closeunusedtabs_reloaded",
    "version": "1.0.0",
    "description": "Every time a new tab is created, all matching tabs will be closed (except the active tab)",
    "permissions": [
        "storage",
        "tabs"
    ],
    "icons": {
        "64": "icons/icon.png"
    },
    "background": {
        "scripts": [
            "src/background/closeunusedtabs.js"
        ]
    },
    "options_ui": {
        "page": "src/options/index.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "addon@example.com"
        }
    }
}