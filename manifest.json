{
    "manifest_version": 2,
    "name": "closeunusedtabs_reloaded",
    "version": "1.0.0",
    "description": "Every time a new tab is created, all matching tabs will be closed (except the active tab)",
    "permissions": [
        "tabs"
    ],
    "icons": {
        "64": "icons/icon.png"
    },
    "background": {
        "scripts": [
            "background/closeunusedtabs.js"
        ]
    },
    "options_ui": {
        "page": "options/index.html"
    }
}